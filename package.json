  "repository": "",
  "bugs": {},
  "engines": {
    "node": ">=8.9.3",
    "npm": ">=5.5.1"
  },
  "dependencies": {
    "-": "^0.0.1",
    "grunt-cli": "^1.4.3"
  },
  "devDependencies": {
    "grunt": "^1.4.1",
    "grunt-checktextdomain": "^1.0.1",
    "grunt-shell": "^3.0.1",
    "grunt-wp-i18n": "^1.0.3",
    "husky": "^7.0.4",
    "matchdep": "^2.0.0"
  },
  "scripts": {
    "postinstall": "husky install",
    "build": "grunt build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.php": [
      "php -d display_errors=1 -l",
      "./vendor/bin/phpcs --extensions=php --warning-severity=8 -s"
    ],
    "*.scss": [
      "stylelint --syntax=scss --fix"
    ],
    "*.{js}": [
      "grunt jshint"
    ]
  },
  "browserslist": [
    "last 2 version",
    "> 1%",
    "IE 10"
  ]
}
